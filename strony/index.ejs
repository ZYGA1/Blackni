<!DOCTYPE html>
<html lang="en" >
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="global.css">
</head>
<% let gej = '' %>
<body>
    <nav>
        <div>
            <h1 id="logo">Jebanie czarnych</h1>
        </div>

        <div id="cont-dodaj">
            <h1 id="dodaj"><a href="/dodaj">Dodaj geja</a></h1>
            <img src="arrow.png" alt="strzalka" >
        </div>
    </nav>
    <div id="con">
        <div id="czarni">
            <h1>JEBAĆ <br> CZARNYCH</h1>
            <p style="margin-top: 30px;"><%= ips.slice(7) %></p>
        </div>

        <div>
            <h1 id="Ostatnie">Ostatnie</h1>
            <% result.forEach(i => {  %>
                <% if(i.gej == 1){gej = 'Pedał'}else{gej = 'Sigma'} %>
                
            <p><%= i.imie  %> <%= i.nazwisko %>,<br> <%= gej %> </p>
            <hr>
            <%})  %>
            
        </div>
    </div>
</body>
</html>